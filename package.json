{
	"name": "amcef",
	"version": "1.0.0",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"dev": "cross-env DB_ENV=dev npx tsx src/server.ts",
		"test": "cross-env DB_ENV=test npx tsx src/server.ts",
		"prod": "cross-env DB_ENV=prod npx tsx src/server.ts",
		"kill-server": "sudo lsof -i :8888 | awk 'NR > 1 {print $2}' | xargs kill -9",
		"db-create-dev": "npx sequelize db:create --env=development",
		"db-migrate-dev": "npx sequelize db:migrate --env=development",
		"db-seed-dev": "npx sequelize db:seed:all --env=development",
		"db-init-dev": "npm run db-drop-dev && npm run db-create-dev && npm run db-migrate-dev && npm run db-seed-dev",
		"db-drop-dev": "npx sequelize db:drop --env=development",
		"db-create-test": "npx sequelize db:create --env=test",
		"db-migrate-test": "npx sequelize db:migrate --env=test",
		"db-seed-test": "npx sequelize db:seed:all --env=test",
		"db-init-test": "npm run db-drop-test && npm run db-create-test && npm run db-migrate-test && npm run db-seed-test",
		"db-drop-test": "npx sequelize db:drop --env=test",
		"db-create-prod": "npx sequelize db:create --env=production",
		"db-migrate-prod": "npx sequelize db:migrate --env=production",
		"db-seed-prod": "npx sequelize db:seed:all --env=production",
		"db-init-prod": "npm run db-drop-prod && npm run db-create-prod && npm run db-migrate-prod && npm run db-seed-prod",
		"db-drop-prod": "npx sequelize db:drop --env=production",
		"db-seq-auto": "npx sequelize-auto -o ./models -l ts esm -d database_test -h 127.0.0.1 -u root -x Root12345 -e mysql"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "`git clone https://github.com/Pojzo/amcef` <br>",
	"dependencies": {
		"@types/jest": "^29.5.12",
		"@types/jsonwebtoken": "^9.0.6",
		"bcrypt": "^5.1.1",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"express": "^4.19.2",
		"express-validator": "^7.2.0",
		"jsonwebtoken": "^9.0.2",
		"mysql2": "^3.11.0",
		"nodemon": "^3.1.4",
		"sequelize": "^6.37.3",
		"sequelize-auto": "^0.8.8",
		"sequelize-cli": "^6.6.2",
		"sequelize-typescript": "^2.1.6",
		"tsx": "^4.17.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.21",
		"@types/sequelize": "^4.28.20",
		"babel-plugin-module-resolver": "^5.0.2",
		"cypress": "^13.13.3",
		"jest": "^29.7.0",
		"supertest": "^7.0.0"
	},
	"directories": {
		"doc": "doc",
		"test": "tests"
	}
}
